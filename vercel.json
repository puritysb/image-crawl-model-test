{
  "rewrites": [
    {
      "source": "/api/crawl/:path*",
      "destination": "/api/main.py"
    },
    {
      "source": "/api/crawl/jobs/:path*",
      "destination": "/api/main.py"
    },
    {
      "source": "/api/health",
      "destination": "/api/main.py"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "python@3.12"
    }
  }
}
